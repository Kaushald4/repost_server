{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "root": "apps",
  "sourceRoot": "apps/repost-apigateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": ["**/*.proto"],
    "watchAssets": true
  },
  "monorepo": true,
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "chat-service": {
      "type": "application",
      "root": "apps/chat-service",
      "entryFile": "main",
      "sourceRoot": "apps/chat-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-service/tsconfig.app.json"
      }
    },
    "comment-service": {
      "type": "application",
      "root": "apps/comment-service",
      "entryFile": "main",
      "sourceRoot": "apps/comment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comment-service/tsconfig.app.json"
      }
    },
    "community-service": {
      "type": "application",
      "root": "apps/community-service",
      "entryFile": "main",
      "sourceRoot": "apps/community-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/community-service/tsconfig.app.json"
      }
    },
    "interaction-service": {
      "type": "application",
      "root": "apps/interaction-service",
      "entryFile": "main",
      "sourceRoot": "apps/interaction-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/interaction-service/tsconfig.app.json"
      }
    },
    "media-service": {
      "type": "application",
      "root": "apps/media-service",
      "entryFile": "main",
      "sourceRoot": "apps/media-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/media-service/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "post-service": {
      "type": "application",
      "root": "apps/post-service",
      "entryFile": "main",
      "sourceRoot": "apps/post-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post-service/tsconfig.app.json"
      }
    },
    "repost-apigateway": {
      "type": "application",
      "root": "apps/repost-apigateway",
      "entryFile": "main",
      "sourceRoot": "apps/repost-apigateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/repost-apigateway/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}
